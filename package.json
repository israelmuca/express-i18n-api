{
  "name": "node-i18n-example",
  "version": "0.0.1",
  "description": "Small example for a i18n in node tutorial.",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && node ./dist/server.js",
    "test": "mocha --require @babel/register ./dist/tests/*.js --timeout 15000 -s 0",
    "clean": "rm -rf dist && mkdir dist",
    "compile": "babel -d ./dist ./src",
    "build": "npm run clean && npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/israelmuca/node-i18n-validation-example.git"
  },
  "keywords": [
    "node",
    "express",
    "i18n",
    "polyglot",
    "tutorial",
    "example"
  ],
  "author": "IsraelMuCa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/israelmuca/node-i18n-validation-example/issues"
  },
  "homepage": "https://github.com/israelmuca/node-i18n-validation-example#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "express-locale": "^1.0.5",
    "express-validator": "^5.3.1",
    "node-polyglot": "^2.3.0",
    "object.fromentries": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "mocha": "^6.0.2"
  }
